---
import LayoutSection from "../layouts/LayoutSection.astro"
---

<LayoutSection
  title="Accesorios"
  color="black"
  id="accesorios"
  
>
  <div slot="footer" class="gap-x-6 flex mx-auto">

    <a
      class="border-[3px] border-gray-800 bg-gray-800 text-sm rounded font-medium text-white px-16 py-2 inline-block transition-colors hover:border-gray-500 hover:bg-gray-500"
      href="#"
    >
      Comprar Ahora</a
    >
  </div>

  <img
    slot="background"
    alt="Model Y"
    class="h-full w-full object-cover object-center bg-image"
    src="accessories.avif"
    id="bg-image-modely"
  />
</LayoutSection>


